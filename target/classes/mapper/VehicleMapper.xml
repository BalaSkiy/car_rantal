<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.VehicleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.entity.Vehicle">
        <id column="vid" property="vid" />
        <result column="license" property="license" />
        <result column="typeid" property="typeid" />
        <result column="dayRate" property="dayrate" />
        <result column="onShelf" property="onshelf" />
        <result column="isRenting" property="isrenting" />
        <result column="repair" property="repair" />
        <result column="remark1" property="remark1" />
        <result column="remark2" property="remark2" />

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        vid, license, typeid, dayRate, onShelf, isRenting, repair, remark1, remark2
    </sql>

</mapper>
