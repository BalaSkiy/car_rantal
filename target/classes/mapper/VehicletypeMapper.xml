<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.VehicletypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.entity.Vehicletype">
        <id column="typeid" property="typeid" />
        <result column="brand" property="brand" />
        <result column="typename" property="typename" />
        <result column="typedescription" property="typedescription" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        typeid, brand, typename, typedescription
    </sql>

</mapper>
